<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>FortunePortlet</display-name>

    <description>Automated generated Application Wrapper</description>

    <servlet>
        <servlet-name>BookManagerPortlet</servlet-name>
        <display-name>BookManagerPortlet Portlet</display-name>
        <description>Generated Portlet Wrapper</description>
        <servlet-class>org.exoplatform.portlet.example.BookManagerPortlet</servlet-class>

        <init-param>
            <param-name>portlet-class</param-name>
            <param-value>org.exoplatform.portlet.example.BookManagerPortlet</param-value>
        </init-param>

        <init-param>
            <param-name>portlet-guid</param-name>
            <param-value>yourportlet.BookManagerPortlet</param-value>
        </init-param>
    </servlet>

    <servlet-mapping>
        <servlet-name>BookManagerPortlet</servlet-name>
        <url-pattern>/BookManagerPortlet/*</url-pattern>
    </servlet-mapping>


</web-app>
