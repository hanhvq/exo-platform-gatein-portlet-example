<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
                         "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>FortunePortlet</display-name>

    <description>Automated generated Application Wrapper</description>
	<listener>
		<listener-class>org.exoplatform.services.portletcontainer.impl.servlet.PortletApplicationListener</listener-class>
	</listener>
    <servlet>
        <servlet-name>BookManagerPortlet</servlet-name>
        <display-name>BookManagerPortlet Portlet</display-name>
        <description>Generated Portlet Wrapper</description>
		<servlet-class>org.exoplatform.services.portletcontainer.impl.servlet.ServletWrapper</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>BookManagerPortlet</servlet-name>
        <url-pattern>/BookManagerPortlet/*</url-pattern>
    </servlet-mapping>


</web-app>
